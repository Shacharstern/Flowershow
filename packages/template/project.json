{
  "name": "template",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/template",
  "projectType": "library",
  "targets": {
    "install": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npm install --prefix packages/template"
      }
    },
    "build": {
      "executor": "@nrwl/next:build",
      "outputs": ["{options.outputPath}"],
      "dependsOn": [{ "projects": "self", "target": "install" }],
      "defaultConfiguration": "production",
      "options": {
        "root": "packages/template",
        "outputPath": "dist/packages/template"
      },
      "configurations": {
        "development": {
          "outputPath": "packages/template"
        },
        "production": {}
      }
    },
    "serve": {
      "executor": "@nrwl/next:server",
      "defaultConfiguration": "development",
      "dependsOn": [{ "projects": "self", "target": "install" }],
      "options": {
        "buildTarget": "template:build",
        "dev": true
      },
      "configurations": {
        "development": {
          "buildTarget": "template:build:development",
          "dev": true
        },
        "production": {
          "buildTarget": "template:build:production",
          "dev": false
        }
      }
    },
    "export": {
      "executor": "@nrwl/next:export",
      "dependsOn": [{ "projects": "self", "target": "install" }],
      "options": {
        "buildTarget": "template:build:production"
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "packages/template/jest.config.js",
        "passWithNoTests": true
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["packages/template/**/*.{ts,tsx,js,jsx}"]
      }
    }
  },
  "tags": []
}
